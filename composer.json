{
  "name": "kjdion84/turtle",
  "description": "A Laravel 5.5 package with front & backend scaffolding including a CRUD generator, auth integration, roles, permissions, contact forms, reCAPTCHA, activity logs, demo mode, user timezones, AJAX CRUD/validation, Bootstrap 4, DataTables, & more!",
  "homepage": "https://github.com/kjdion84/turtle",
  "license": "MIT",
  "authors": [
    {
      "name": "Kevin Dion",
      "email": "kjdion84@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "google/recaptcha": "~1.1",
    "graham-campbell/throttle": "~6.0",
    "laravel/framework": "5.5.*",
    "yajra/laravel-datatables-oracle": "~8.0"
  },
  "autoload": {
    "psr-4": {
      "Kjdion84\\Turtle\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kjdion84\\Turtle\\TurtleServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-package-install": [
      "php artisan vendor:publish --provider=Kjdion84\\Turtle\\TurtleServiceProvider --tag=config",
      "php artisan vendor:publish --provider=Kjdion84\\Turtle\\TurtleServiceProvider --tag=layout",
      "php artisan vendor:publish --provider=Kjdion84\\Turtle\\TurtleServiceProvider --tag=public"
    ],
    "post-package-update": [
      "php artisan vendor:publish --provider=Kjdion84\\Turtle\\TurtleServiceProvider --tag=public --force"
    ]
  }
}